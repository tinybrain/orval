  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach to Process",
      "type": "node",
      "request": "attach",
      "processId": "${command:PickProcess}"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/dist/bin/orval.js",
      "cwd": "${workspaceFolder}",
      "args": ["--config", "../../../ciss/cie-api-orval/orval.config.ts"],
      "preLaunchTask": "npm: build-dev",
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
